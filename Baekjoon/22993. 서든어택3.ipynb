    "n = int(input())\n",
    "fp = list(map(int, input().split()))\n",
    "\n",
    "hero = fp[0]\n",
    "fp = sorted(fp[1:])\n",
    "win = True\n",
    "\n",
    "for i in range(n - 1) :\n",
    "    if hero > fp[i] :\n",
    "        hero += fp[i]\n",
    "    else :\n",
    "        win = False\n",
    "        break\n",
    "        \n",
    "if win :\n",
    "    print(\"Yes\")\n",
    "else :\n",
    "    print(\"No\")\n",
    "    "
   ]
